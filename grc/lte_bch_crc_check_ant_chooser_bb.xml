<?xml version="1.0"?>
<block>
  <name>BCH CRC antenna selector</name>
  <key>lte_bch_crc_check_ant_chooser_bb</key>
  <category>lte</category>
  <import>import lte</import>
  <make>lte.bch_crc_check_ant_chooser_bb("$id")</make>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in1</name>
    <type>byte</type>
  </sink>

  <sink>
    <name>in2</name>
    <type>byte</type>
  </sink>
  
    <sink>
    <name>in4</name>
    <type>byte</type>
  </sink>
  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>N_ant</name>
    <type>byte</type>
  </source>
</block>
